  </xns1:production>
  <xns1:production>
    <nonterminal>Phase</nonterminal>
    <xns1:expression>
      <sequence>
	<xns1:expression>
	  <plus>
	    <xns1:expression>
	      <selectable>
		<selector>artifactUsageNames</selector>
		<xns1:expression>
