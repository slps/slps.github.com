  </xns1:production>
  <xns1:production>
    <nonterminal>WorkDefinition</nonterminal>
    <xns1:expression>
      <nonterminal>Phase</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>WorkDefinition</nonterminal>
    <xns1:expression>
      <nonterminal>Lifecycle</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>WorkDefinition</nonterminal>
    <xns1:expression>
