      <nonterminal>WorkDefinition</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>RoleKind</nonterminal>
    <xns1:expression>
      <plus>
	<xns1:expression>
	  <selectable>
	    <selector>artifactKinds</selector>
	    <xns1:expression>
	      <nonterminal>ArtifactKind</nonterminal>
	    </xns1:expression>
	  </selectable>
	</xns1:expression>
      </plus>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>Condition</nonterminal>
    <xns1:expression>
      <nonterminal>Goal</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>Condition</nonterminal>
    <xns1:expression>
      <nonterminal>Precondition</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>Condition</nonterminal>
