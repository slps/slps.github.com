	    </xns1:expression>
	  </selectable>
	</xns1:expression>
      </plus>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>Goal</nonterminal>
    <xns1:expression>
      <selectable>
	<selector>workDefinition</selector>
	<xns1:expression>
	  <nonterminal>WorkDefinition</nonterminal>
	</xns1:expression>
      </selectable>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>Precondition</nonterminal>
