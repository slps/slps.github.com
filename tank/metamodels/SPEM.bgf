      <selectable>
	<selector>workDefinition</selector>
	<xns1:expression>
	  <nonterminal>WorkDefinition</nonterminal>
	</xns1:expression>
      </selectable>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>WorkDefinition</nonterminal>
    <xns1:expression>
      <nonterminal>ActivityKind</nonterminal>
    </xns1:expression>
  </xns1:production>
  <xns1:production>
    <nonterminal>WorkDefinition</nonterminal>
    <xns1:expression>
