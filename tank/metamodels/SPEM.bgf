	<xns1:expression>
	  <selectable>
	    <selector>goal</selector>
	    <xns1:expression>
	      <nonterminal>Goal</nonterminal>
	    </xns1:expression>
	  </selectable>
	</xns1:expression>
	<xns1:expression>
	  <selectable>
	    <selector>precondition</selector>
	    <xns1:expression>
	      <nonterminal>Precondition</nonterminal>
	    </xns1:expression>
	  </selectable>
