	<xns1:expression>
	  <nonterminal>Apply</nonterminal>
	</xns1:expression>
      </choice></bgf:expression></bgf:production><bgf:production><nonterminal xmlns:xns1="http://planet-sl.org/bgf">Literal</nonterminal><bgf:expression><selectable xmlns:xns1="http://planet-sl.org/bgf">
