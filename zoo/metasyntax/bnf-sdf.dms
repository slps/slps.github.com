Rules = Rule* ;
Rule = NonTerminal '::=' Elements ;
NonTerminal = '<' STR '>' ;
Elements = Element ('|' Element)* ;
Element = NonTerminal ;
Element = Terminal ;
Element = '[' Element ('|' Element)* ']' ;
Element = '{' Element ('|' Element)* '}' ;
Element = '|' ;
Element = '[' ;
Element = ']' ;
Element = '{' ;
Element = '}' ;
Element = '<' ;
Element = '>' ;
Element = '...' ;
Element = '::=' ;
Element = Element Element ;
Terminal = STR ;
